#include <stdio.h>

// hcf(gcd) of two numbers 

int main()
{
    /*
    int a,b,i,hcf;
    printf("Enter two integers\n");
    scanf("%d%d",&a,&b);
    
    for(i=1;i<=a||i<=b; i++){
        if(a%i==0 && b%i==0){
            hcf=i;
        }
    }
    printf("hcf is %d",hcf);
    
    
    // lcm of two numbers
    
    int a,b,i,hcf,lcm;
    printf("Enter two numbers \n");
    scanf("%d%d",&a,&b);
    for(i=1; i<=a||i<=b; i++){
        if(a%i==0 && b%i==0){
            hcf=i;
        }
    }
    lcm = (a*b)/hcf;
    printf("lcm of %d and %d is %d",a,b,lcm);
    
    return 0;
    }
