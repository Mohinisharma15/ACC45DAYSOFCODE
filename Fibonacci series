// Fibonacci series 
#include <stdio.h>
int main(){

int i,n, t1,t2,nextterm;

t1 = 0;
t2 = 1;
nextterm = t1 + t2;
printf("Enter the number \n");
scanf("%d", &n);

printf("Fabnacci series is %d %d ", t1,t2);

for(i=3; i<=n; i++ ){
    printf("%d ", nextterm );

    t1 = t2;
    t2 = nextterm;
    nextterm = t1 +t2;

}
return 0;
}
